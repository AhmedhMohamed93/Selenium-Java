<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite">
  <!-- Set browser parameter (ie = internet explorer, chrome = Google chrome -->
  <parameter name="browser" value="chrome" />
  <test thread-count="5" name="Test1">
	<classes>
			<class name="test.ValidateLoginAndLaunch">
				<methods>
					<exclude name="BD_ValidateLaunchWebsite"/>
					<exclude name="BD_LoginSuccessfully"/>
				</methods>
			</class>
	 
			<class name="test.ValidateCreateNewIDN">
				<methods>
					<exclude name="BD_ValidateCreationOfNewHealthSystem"/>
				</methods>
			</class> 
	
			<class name="test.ValidateCreateNewfacility">
				<methods>
					<exclude name="BD_ValidateCreationOfNewFacility"/>
				</methods>
			</class>

			<class name="test.ValidateCreateNewPF">
				<methods>
					<exclude name="BD_ValidateCreationOfNewPharmacyFormualry"/>
				</methods>
			</class>

			<class name="test.ValidateCreateAlignmentProject">
				<methods>
					<include name="BD_ValidateCreationOfNewAlignmentProject"/>
				</methods>
			</class>			
		
	</classes>
  </test>
</suite> 
